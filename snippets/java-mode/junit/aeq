# name: aeq
# --
${1:String} $2 = ${1:$(cond ((string= "String" text) "\"\"")
                            ((string= "int" text) "0")
                            ((string= "long" text) "0L")
                            (t "null"))};
$1 $3 = ${1:$(cond ((string= "String" text) "\"\"")
                            ((string= "int" text) "0")
                            ((string= "long" text) "0L")
                            (t "null"))};
assertEqual(${2:expected}, ${3:actual});
